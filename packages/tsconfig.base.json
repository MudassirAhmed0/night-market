{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["ES2023", "DOM"],
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react-jsx",
      "baseUrl": ".",
      "paths": {
        "@/*": ["apps/frontend/*"]
      }
    }
  }
  